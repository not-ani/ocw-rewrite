{
  "name": "@ocw/validators",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ocw/eslint-config": "workspace:*",
    "@ocw/prettier-config": "workspace:*",
    "@ocw/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:"
  }
}
