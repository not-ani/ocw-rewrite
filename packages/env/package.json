{
  "name": "@ocw/env",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./web": "./src/web.ts",
    "./server": "./src/server.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.10",
    "@t3-oss/env-nextjs": "^0.13.1",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@ocw/eslint-config": "workspace:*",
    "@ocw/prettier-config": "workspace:*",
    "@ocw/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:"
  }
}
